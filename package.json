{
  "name": "maptalks.formats",
  "version": "0.1.0",
  "description": "a geospatial format parser for Maptalks",
  "main": "index.js",
  "scripts": {
    "test-local": "zuul --local -- test/test.js",
    "test-remote": "zuul -- test/test.js",
    "test": "zuul --phantom -- test/test.js",
    "build": "browserify --external maptalks index.js > maptalks.formats.js && uglifyjs maptalks.formats.js -c -m > maptalks.formats.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maptalks/maptalks.formats.git"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "keywords": [
    "maptalks",
    "formats",
    "kml",
    "csv",
    "gpx",
    "geojson",
    "kml",
    "maps",
    "gpx",
    "wkt",
    "osm",
    "polyline",
    "topojson",
    "format",
    "converter"
  ],
  "author": [
    "Tom MacWright",
    "Fu Zhen"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/maptalks/maptalks.formats/issues"
  },
  "homepage": "https://github.com/maptalks/maptalks.formats",
  "dependencies": {
    "brfs": "1.4.3",
    "corslite": "0.0.7",
    "csv2geojson": "~5.0.0",
    "polyline": "0.2.0",
    "togeojson": "0.13.0",
    "topojson": "1.6.26",
    "wellknown": "0.4.2"
  },
  "devDependencies": {
    "browserify": "13.0.1",
    "eslint": "^3.4.0",
    "maptalks": "<2.0.0",
    "mocha": "~2.5.3",
    "phantomjs-prebuilt": "2.1.7",
    "st": "1.1.0",
    "tape": "4.5.1",
    "uglify-js": "^2.6.2",
    "zuul": "~3.10.1"
  }
}
